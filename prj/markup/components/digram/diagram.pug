mixin diagram(deg,name,color)
    .deg.animated.fadeInUp.wow
        svg.over-image(viewport="0 0 250 250",class=color)
            circle.back(r="100",cx="120",cy="120",fill="transparent",stroke-dasharray="628.318530718",stroke-dashoffset=((100-deg)/100)*628.318530718)
            circle#bar(r="108",cx="120",cy="120",fill="transparent",stroke-dasharray="678.584013175",stroke-dashoffset=((100-deg)/100)*678.584013175)


        span=deg+"%"
        p.name_skill=name



